<odoo>
    <data>
        <!-- Minuta de reunión Report -->
        <record id="action_report_record_meeting" model="ir.actions.report">
            <field name="name">Minuta de reunión</field>
            <field name="model">record.meeting</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">mgmtsystem_comunication.record_meeting_report_template</field>
            <field name="report_file">mgmtsystem_comunication.record_meeting_report_template</field>
            <field name="paperformat_id" ref="hola_calidad.portrait"/>
        </record>

        <!-- Reporte de Convocatoria -->
        <record id="action_report_record_conv_meeting" model="ir.actions.report">
            <field name="name">Reporte de Convocatoria</field>
            <field name="model">record.meeting</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">mgmtsystem_comunication.record_meeting_con</field>
            <field name="report_file">mgmtsystem_comunication.record_meeting_con</field>
            <field name="print_report_name">'Reporte de Convocatoria - %s - %s' % (object.name, object.date_release.strftime('%Y-%m-%d'))</field>
            <field name="paperformat_id" ref="hola_calidad.portrait"/>
        </record>

        <!-- Reporte de Asistencia -->
        <record id="action_report_record_ass_meeting" model="ir.actions.report">
            <field name="name">Reporte de Asistencia</field>
            <field name="model">record.meeting</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">mgmtsystem_comunication.record_meeting_assist</field>
            <field name="report_file">mgmtsystem_comunication.record_meeting_assist</field>
            <field name="paperformat_id" ref="hola_calidad.portrait"/>
        </record>
        <template id="record_meeting_report_template">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-if="doc and 'company_id' in doc">
                        <t t-set="company" t-value="doc.company_id" />
                    </t>
                    <t t-if="not doc or not 'company_id' in doc">
                        <t t-set="company" t-value="res_company" />
                    </t>
                    <div class="header">
                        <style type="text/css">
                            .tg.white-bg td {
                                background-color: #ffffff; /* Fondo blanco para la primera tabla */
                                }
                            .tg td{border-color:black;border-style:solid;border-width:1px;font-family: Palatino, Palatino Linotype, Palatino LT STD, Book Antiqua, Georgia, serif;font-size:14px;
                            overflow:hidden;padding:10px 5px;word-break:normal;text-align:middle;align:center;}
                            .tg th{border-color:black;border-style:solid;border-width:1px;font-family: Palatino, Palatino Linotype, Palatino LT STD, Book Antiqua, Georgia, serif;font-size:14px;
