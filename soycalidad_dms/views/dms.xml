<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_dms_file_form" model="ir.ui.view">
        <field name="name">dms.file.view.form.inherit</field>
        <field name="model">dms.file</field>
        <field name="inherit_id" ref="dms.view_dms_file_form"/>
        <field name="arch" type="xml">
            <field name="tag_ids" position="after">
                <field name="language" />
                <field name="version" />
            </field>
            <field name="content" position="after">
                <field name="url" />
            </field>
        </field>
    </record>
</odoo>
