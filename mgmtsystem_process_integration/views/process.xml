        <record id="mgmt_categ_form_inherit" model="ir.ui.view">
            <field name="name">mgmt.categ.view.form.inherit</field>
            <field name="model">mgmt.categ</field>
            <field name="inherit_id" ref="mgmtsystem_process.mgmt_categ_form" />
            <field name="arch" type="xml">
                <xpath expr="//page[1]" position="before">
                    <page name="process" string="Proceso">
                        <group string="Proceso" col="2">
                            <separator string="Recursos" colspan="2" />
                            <field name="resource_ids" nolabel="1" colspan="2">
                                <tree editable="bottom">
                                    <field name="type_id" />
                                    <field name="name" />
                                    <field name="description" />
                                </tree>
                            </field>
                            <separator string="Entradas" colspan="2" />
                            <field name="process_line_in_ids" nolabel="1" colspan="2">
                                <tree editable="bottom">
                                    <field name="supplier_id" widget="many2many_tags" context="{'default_type': 'input'}" />
                                    <field name="input_id" widget="many2many_tags" />
                                </tree>
                            </field>
                            <separator string="Actividades" colspan="2" />
                            <separator string="Planificar" colspan="2" />
                            <field name="interaction_plan" context="{'default_type': 'plan'}" colspan="2" nolabel="1">
                                <tree string="">
                                    <field name="type" invisible="1" />
                                    <field name="name" style="width:200px;max-width:200px;" />
                                </tree>
                            </field>
                            <separator string="Hacer" colspan="2" />
                            <field name="interaction_do" context="{'default_type': 'do'}" colspan="2" nolabel="1">
                                <tree string="">
                                    <field name="type" invisible="1" />
                                    <field name="name" style="width:200px;max-width:200px;" />
                                </tree>
                            </field>
                            <separator string="Verificar" colspan="2" />
                            <field name="interaction_verify" context="{'default_type': 'verify'}" colspan="2" nolabel="1">
                                <tree string="">
                                    <field name="type" invisible="1" />
                                    <field name="name" style="width:200px;max-width:200px;" />
                                </tree>
                            </field>
                            <separator string="Actuar" colspan="2" />
                            <field name="interaction_act" context="{'default_type': 'act'}" colspan="2" nolabel="1">
                                <tree string="">
                                    <field name="type" invisible="1" />
                                    <field name="name" style="width:200px;max-width:200px;" />
                                </tree>
                            </field>
