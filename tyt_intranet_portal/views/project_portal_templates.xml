<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="portal_my_home_project" inherit_id="project.portal_my_home" priority="40">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="before">
            <t t-set="portal_service_category_enable" t-value="True"/>
        </xpath>
        <div id="portal_service_category" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="icon" t-value="'/tyt_intranet_portal/static/src/img/folder.svg'"/>
                <t t-set="title">Proyectos</t>
                <t t-set="url" t-value="'/my/projects'"/>
                <t t-set="text">Siga la evoluci√≥n de sus proyectos</t>
            </t>
            <t t-call="portal.portal_docs_entry">
                <t t-set="icon" t-value="'/tyt_intranet_portal/static/src/img/tasks.svg'"/>
                <t t-set="title">Tareas</t>
                <t t-set="url" t-value="'/my/tasks'"/>
                <t t-set="text">Siga y comente las tareas de sus proyectos</t>
            </t>
        </div>
    </template>
</odoo>
