<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="survey_user_input_view_tree" model="ir.ui.view">
        <field name="name">survey_user_input.view.tree.inherit</field>
        <field name="model">survey.user_input</field>
        <field name="inherit_id" ref="survey.survey_user_input_view_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='survey_id']" position="after">
                <field name="employee_number"/>
            </xpath>
        </field>
    </record>
</odoo>
