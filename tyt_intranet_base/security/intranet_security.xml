<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="module_category_intranet" model="ir.module.category">
        <field name="name">Intranet</field>
        <field name="sequence">50</field>
    </record>

    <record id="group_intranet_manager" model="res.groups">
        <field name="name">Intranet Manager</field>
        <field name="category_id" ref="module_category_intranet"/>
        <field name="users" eval="[(4, ref('base.user_root')),(4, ref('base.user_admin'))]"/>
    </record>

    <record id="group_intranet_user" model="res.groups">
        <field name="name">Intranet User</field>
        <field name="category_id" ref="module_category_intranet"/>
    </record>
</odoo>
