    <record id="complaint_complaint_view_form" model="ir.ui.view">
        <field name="name">complaint.complaint.view.form</field>
        <field name="model">complaint.complaint</field>
        <field name="inherit_id" ref="mgmtsystem_complaints.view_complaint_complaint_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='button_box']" position="inside">
                <button name="open_change_request_view" type="object" class="oe_stat_button" icon="fa-book">
                    <field name="change_requests_count" widget="statinfo" string="Solicitudes de cambio"/>
                </button>
            </xpath>
            <xpath expr="//notebook" position="inside">
                <page string="Solicitudes de cambio">
                    <field name="change_request_ids"/>
