<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_report_procedure_edition_tyt" model="ir.actions.report">
        <field name="name">Procedure Edition</field>
        <field name="model">process.edition</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">tyt_process.report_procedure_edition_tyt</field>
        <field name="report_file">tyt_process.report_procedure_edition_tyt</field>
        <field name="print_report_name">'%s' % object.name</field>
        <field name="binding_model_id" ref="model_process_edition"/>
        <field name="binding_type">report</field>
    </record>

    <template id="report_procedure_edition_tyt">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                </t>
            </t>
        </t>
    </template>

    <record id="mgmtsystem_process.report_process_edition" model="ir.actions.report">
        <field name="binding_model_id" eval="False" />
    </record>

</odoo>
