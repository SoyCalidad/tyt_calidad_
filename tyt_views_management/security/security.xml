<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>

        <record id="category_tyt_user" model="ir.module.category">
            <field name="name">TYT and Frescos User</field>
        </record>
    
        <record id="group_tyt_user" model="res.groups">
            <field name="name">TYT User</field>
            <field name="category_id" ref="category_tyt_user" />
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]" />
        </record>

        <record id="group_frescos_user" model="res.groups">
            <field name="name">Frescos User</field>
            <field name="category_id" ref="category_tyt_user" />
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]" />
        </record>
    
    </data>

</odoo>
